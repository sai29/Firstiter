<%= form_for(@project) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <br>
  <div class="large-4 columns ">
  	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :tagline%>
	<%= f.text_field :tagline %>

    <%= f.text_area :description, rows: 6, placeholder: "Describe the project..." %>
  
  <%= f.submit "Post", class: "button small radius" %>
</div>
<% end %>